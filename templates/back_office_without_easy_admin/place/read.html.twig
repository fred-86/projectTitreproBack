{% extends 'base.html.twig' %}

{% block title %}Place{% endblock %}

{% block body %}
    <h1>Place</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ place.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ place.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ place.address }}</td>
            </tr>
            <tr>
                <th>AddressComplement</th>
                <td>{{ place.addressComplement }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ place.city }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ place.logo }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ place.image }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ place.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ place.createdAt ? place.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ place.updatedAt ? place.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>PublishedAt</th>
                <td>{{ place.publishedAt ? place.publishedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ place.url }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ place.content }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_office_place_browse') }}">back to list</a>

    <a href="{{ path('back_office_place_edit', {'id': place.id}) }}">edit</a>

    {{ include('back_office_without_easy_admin/place/_delete_form.html.twig') }}
{% endblock %}
